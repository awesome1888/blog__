{
    "name": "gatsby-starter-styled-components",
    "private": true,
    "description": "A simple starter to get up and developing quickly with Gatsby and styled-components",
    "version": "0.1.0",
    "author": "Blake Noll <nollblake@gmail.com>",
    "dependencies": {
        "babel-plugin-styled-components": "^1.10.0",
        "gatsby": "^2.1.18",
        "gatsby-image": "^2.0.30",
        "gatsby-plugin-manifest": "^2.0.19",
        "gatsby-plugin-offline": "^2.0.24",
        "gatsby-plugin-react-helmet": "^3.0.7",
        "gatsby-plugin-sharp": "^2.0.22",
        "gatsby-plugin-styled-components": "^3.0.6",
        "gatsby-source-filesystem": "^2.0.23",
        "gatsby-transformer-sharp": "^2.1.14",
        "prop-types": "^15.7.2",
        "react": "^16.8.3",
        "react-dom": "^16.8.3",
        "react-helmet": "^5.2.0",
        "sc-companion": "^2.1.7",
        "sharp": "^0.30.5",
        "styled-components": "^4.1.3"
    },
    "devDependencies": {
        "eslint": "^5.16.0",
        "eslint-config-airbnb-base": "^13.1.0",
        "eslint-config-prettier": "^4.3.0",
        "eslint-plugin-import": "^2.17.3",
        "eslint-plugin-jsx-a11y": "^6.2.1",
        "eslint-plugin-prettier": "^3.1.0",
        "eslint-plugin-react": "^7.13.0",
        "eslint-plugin-react-hooks": "^1.6.0",
        "gh-pages": "^2.0.1",
        "husky": "^2.3.0",
        "prettier": "^1.17.1",
        "pretty-quick": "^1.11.0"
    },
    "keywords": [
        "gatsby"
    ],
    "license": "MIT",
    "scripts": {
        "build": "gatsby build",
        "develop": "gatsby develop --host=0.0.0.0",
        "deploy": "gatsby build --prefix-paths && gh-pages -d public",
        "format": "prettier --write src/**/*.{js,jsx}",
        "start": "npm run develop",
        "serve": "gatsby serve",
        "test": "echo \"Write tests! -> https://gatsby.app/unit-testing\""
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/blakenoll/gatsby-starter-styled-components"
    },
    "bugs": {
        "url": "https://github.com/blakenoll/gatsby-starter-styled-components/issues"
    },
    "husky": {
        "hooks": {
            "pre-commit": "pretty-quick --staged"
        }
    }
}
